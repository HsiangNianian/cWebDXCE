{
  "version": 2,
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": ".*",
      "dest": "/app.py"
    }
  ],
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "env": {
    "PRODUCTION": "true",
    "FLASK_ENV": "production",
    "SQLITE_DATABASE_URI": "sqlite:///database/data.db"
  }
}