{
  "version": 2,
  "routes": [
    {
      "src": "/api/dice",
      "dest": "api/dice.py"
    },
    {
      "src": "/api/roll",
      "dest": "api/roll.py"
    },
    {
      "src": "/(.*)",
      "dest": "static/index"
    }
  ],
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ]
}
