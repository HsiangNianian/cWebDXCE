{
  "version": 2,
  "functions": {
    "api/dice.py": {
      "memory": 3008,
      "maxDuration": 60
    },
    "functions": {
      "api/roll.py": {
        "memory": 3008,
        "maxDuration": 60
      },
    "routes": [
      {
        "src": "/api/dice",
        "dest": "api/dice.py"
      },
      {
        "src": "/api/roll",
        "dest": "api/roll.py"
      },
      {
        "src": "/(.*)",
        "dest": "static/index"
      }
    ],
    "builds": [
      {
        "use": "@vercel/python"
      }
    ]
  }